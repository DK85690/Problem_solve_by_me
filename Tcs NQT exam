//question 1 answer

#include <iostream>
using namespace std;

int main()
{
    int T,D;
    char att;
    cin>>T>>D;
    int count=0;
    float per=80.0;
    
    for(int i=0;i<T;i++){
        int p=0;
        float rem=0.0;
        for(int j=0;j<D;j++){
            cin>>att;
            if(att=='P' || att=='p'){
                p++;
            }
        }
        rem=float(p/D)*100.0;
        if(rem>per){
            count++;
        }
    }
    cout<<count;
    return 0;
}
